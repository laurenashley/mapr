<div id="pinsList">
<% if (map) { %>
  <a id="backBtnAllMap" class="btn btn-outline-secondary" href="/maps">Back</a>
  <% map.forEach((item) => { %>
    <h3 class="h6 mt-3"><%= item.title %></h3>
    <% if (userid) { %>
      <a id="newPinBtn" class="btn btn-primary" href="/maps/<%= item.id %>/pins/new" data-mapid="<%= item.id %>">Add pin</a>
    <% } %>
  <% }); %>
  <ul class="nav flex-column mb-3">
    <% if (pins.length > 0) { %>
      <% pins.forEach((pin) => { %>
        <li id="<%= pin.id %>" class="d-flex py-3">
          <img class="pe-2" width="75" height="75" src="<%= pin.image_url %>" />
          <div>
            <h6><a href="/pins/<%= pin.id %>" data-pinid="<%= pin.id %>"><%= pin.title %></a></h6>
            <p><%= pin.description %></p>
          </div>
        </li>
      <% }); %>
    <% } else { %>
      <div class="alert alert-warning">
        No pins found
      </div>
    <% } %>
  </ul>
  
<% } else { %>
  No map found
<% } %>
</div>