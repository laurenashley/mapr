
    <form id="updateMapForm" action="/maps/update" method="post">
      <h3 class="h5">Update MAP_NAME_HERE map</h3>
      <p>
        <label class="form-label" for="mapName">Map name</label>
        <input id="mapName" class="form-control" name="mapName" type="text" value="">
      </p>
      <p>
        <label class="form-label" for="mapLong">Longitude</label>
        <input class="form-control" id="mapLong" name="mapLong" type="text" value="">
      </p>
      <p>
        <label class="form-label" for="mapLat">Latitude</label>
        <input class="form-control" id="mapLat" name="mapLat" type="text" value="">
      </p>
      <p>
        <label class="form-label" for="mapZoom">Zoom</label>
        <input id="mapZoom" class="form-control" name="mapZoom" type="text" value="">
      </p>
      <input class="btn btn-primary" type="submit" name="submit" value="Save">
    </form>
